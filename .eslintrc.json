{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "indent": [
        "error",
        2
    ],
    "quotes": [
        "error",
        "single"
    ],
    "semi": [
        "error",
        "always"
    ],
        "no-underscore-dangle": ["error",{"allow": ["_id"]}],
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
        "consistent-return": "off"
    }
}
