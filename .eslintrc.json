{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-underscore-dangle": ["error",{"allow": ["_id"]}],
      "no-console": "off",
      "object-curly-newline": ["error", { "consistent": true }],
      "import/no-extraneous-dependencies": "off"
    }
}

